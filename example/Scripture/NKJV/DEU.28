        { "type": "p", "content": "" },
        { "type": "note", "content": "2 Sam. 12:11; Job 31:10; Jer. 8:10" },
        { "content": "\u201cYou shall betroth a wife, but another man shall lie with her; " },
        { "type": "note", "content": "Amos 5:11; Zeph. 1:13" },
        { "content": "you shall build a house, but " },
        { "type": "note", "content": "Deut. 20:6; Job 31:8; Jer. 12:13; Mic. 6:15" },
